{
  "name": "eth.faucet",
  "private": true,
  "version": "1.0.0",
  "type": "commonjs",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "abi:gen-interface": "for file in public/**/*.json; do npx abi-types-generator $file --output=src/types/ --provider=web3; done",
    "truffle:migrate": "TMP_CONFIG=/tmp/truffle-config.cjs && cp ./truffle-config.js $TMP_CONFIG && truffle migrate --config $TMP_CONFIG --reset && rm $TMP_CONFIG && yarn run abi:gen-interface"
  },
  "dependencies": {
    "@metamask/detect-provider": "^2.0.0",
    "@metamask/providers": "^10.2.1",
    "bulma": "^0.9.4",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "web3": "^1.9.0",
    "web3-eth-contract": "^1.9.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.192",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.32",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "@vitejs/plugin-react-swc": "^3.2.0",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react": "^7.32.2",
    "ethereum-abi-types-generator": "^1.3.2",
    "ganache": "^7.7.7",
    "prettier": "^2.8.7",
    "truffle": "^5.8.1",
    "typescript": "^5.0.3",
    "vite": "^4.2.1"
  }
}
